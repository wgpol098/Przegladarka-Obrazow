<Window x:Class="Przegladarka_obazow.AppWindow.FastFilter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Przegladarka_obazow.AppWindow"
        mc:Ignorable="d"
        Title="FastFilter" Height="450" Width="800"
        KeyDown="KeyDownUI"
        Closing="FastFilter_Closing">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush StartPoint="0,0" EndPoint="1,0">
                <GradientStop Offset="0" Color="LightGray"/>
                <GradientStop Offset="0.5" Color="Gray"/>
                <GradientStop Offset="1" Color="LightGray"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Image Name="ImageFilterBox" Margin="10,10,0,66" HorizontalAlignment="Stretch" Width="Auto" Stretch="Uniform"/>

    </Grid>
</Window>
